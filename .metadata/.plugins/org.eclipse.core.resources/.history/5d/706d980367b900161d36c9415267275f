package carte;

public class Killinstred extends Carte{
	
	public static Killinstred instance;
	
	private Killinstred(){
		this.nom = "Killinstred";
		this.capacite = "Peut détruire toutes les cartes de Croyants au centre de la table d'Origine Néant.";
		this.estDistribuee = false;
		this.typeCarte = TypeCarte.divinite;
	}
	
	public Killinstred getInstance(){
		if (instance == null) {
			instance = new Killinstred();
		}
		return instance;
	}

}
