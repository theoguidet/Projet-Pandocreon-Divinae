package carte.croyants;

import carte.Carte;
import propriete.Propriete;
/**
 * Croyant hérite de Carte
 * représente un type de carte du jeu
 * @author Admin
 *
 */
public class Croyant extends Carte{
	protected int nbCroyants;
	
	/**
	 * Constructeur de la classe
	 */
	public Croyant(){
		super();
	}

	/**
	 * getter des propriétés de la carte
	 * @return les propriétés de la carte
	 */
	public Propriete getPropriete() {
		return propriete;
	}

	public int getNbCroyants() {
		return nbCroyants;
	}
	
}
