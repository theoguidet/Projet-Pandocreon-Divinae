package carte.croyants;

import propriete.Dogme;
import propriete.Origine;
import propriete.Propriete;
import carte.Carte; 
import carte.TypeCarte;

public class Moines extends Carte{
	
	private Propriete propriete;
	
	public Moines (String nom, String capacite, boolean estDistr, TypeCarte typeCarte, Dogme d1, Dogme d2, Dogme d3, Origine o){
		super(nom, capacite, estDistr, typeCarte);
		Propriete p = new Propriete(d1, d2, d3, o);
		this.propriete = p;
	}
	
	public void sacrifice(){
		
	}
}
